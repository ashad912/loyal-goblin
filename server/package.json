    "babel-plugin-module-resolver": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "email-templates": "^6.1.1",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "express-subdomain": "^1.0.5",
    "express-validator": "^6.6.0",
    "heroku": "^7.29.0",
    "jest": "^24.8.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
